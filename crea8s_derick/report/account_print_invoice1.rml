  <story>
  <pto>
    <para style="terp_default_8">[[ repeatIn(objects,'o') ]]</para>
    <para style="terp_default_8">[[ setLang(o.partner_id.lang) ]]</para>
    <spacer length="2.0cm"/>

      <blockTable colWidths="120mm" style="Table_Partner_Address">
        <tr>
          <td>
	    <para style="terp_headerc">[[ company.street and company.street or '' ]]</para>
	    <para style="terp_headerc">[[ company.street2 and company.street2 or '' ]] [[ company.city and company.city or '' ]] [[ company.zip and company.zip or '' ]]</para>
	    <para style="terp_headerc"> TEL: [[ company.phone and company.phone or '' ]] FAX: [[ company.fax and company.fax or '' ]] </para>
	  </td>
        </tr>
      </blockTable>

	<blockTable colWidths="60mm,60mm" style="Table_Partner_Address">
      <tr>
        <td>
			<para style="terp_default_8_r"><b> RCB No.  </b></para>
		</td>
		<td>
			<para style="terp_default_8"><b> GST REG No.  </b></para>
		</td>
	  </tr>
	</blockTable>
	<para style="terp_header"><b> Tax Invoice </b></para>
	<blockTable colWidths="62mm,9mm,54mm" style="Table_billto">
      <tr>
        <td>
          <para style="terp_default_8"> Bill To: </para>
		  <spacer length="0.25cm"/>
		  <para style="terp_default_8">[[ (o.partner_id and o.partner_id.title and o.partner_id.title.name) or '' ]] [[ (o.partner_id and o.partner_id.name) or '' ]]</para>
		  <para style="terp_default_8">[[ display_address(o.partner_id) ]]</para>
          <para style="terp_default_8">Tel. : [[ (o.partner_id.phone) or removeParentNode('para') ]]</para>
          <para style="terp_default_8">Fax : [[ (o.partner_id.fax) or removeParentNode('para') ]] </para>
		</td>
		<td>
			<para style="terp_default_8"> </para>
		</td>
        <td>
          <para style="terp_default_8">Date : [[ o.date_invoice and time.strftime('%d-%m-%Y',time.strptime(o.date_invoice, '%Y-%m-%d')) or ' ' ]]</para>
		  <para style="terp_default_8">Invoice No : [[ o.number and o.number or ' ' ]]</para>
		  <para style="terp_default_8">Term : [[ o.payment_term and o.payment_term.name or ' ' ]]</para>
		  <para style="terp_default_8">Page : 1</para>
        </td>
      </tr>
    </blockTable>
        <spacer length="0.5cm"/>
    <blockTable colWidths="41mm,21mm,21mm,21mm,21mm" style="Table8">
      <tr>
        <td>
          <para style="terp_tblheader_General_Centre"><b>ITEM</b></para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre"><b>QTY</b></para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre"><b>UOM</b></para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre"><b>UNIT PRICE</b></para>
        </td>
        <td>
          <para style="terp_tblheader_General_Centre"><b>AMOUNT</b></para>
        </td>
      </tr>
    </blockTable>
        <section>
      <para style="terp_default_2">[[ repeatIn(o.invoice_line,'l') ]]</para>
      <blockTable colWidths="41mm,21mm,21mm,21mm,21mm" style="Table8">
        <tr>
          <td>
            <para style="terp_default_9"> [[ format(l.name) ]] </para>
          </td>
          <td>
            <para style="terp_default_Centre_9"> [[ l.qty_others and formatLang(l.qty_others) or ' ' ]] </para>
          </td>
          <td>
		<para style="terp_default_Centre_9"> [[ l.uom_others and l.uom_others.name or (l.uos_id and l.uos_id.name or '') ]] </para>
          </td>
          <td>
            <para style="terp_default_Centre_9">[[ formatLang(l.price_unit) ]]</para>
          </td>
          <td>
            <para style="terp_default_Right_9"> [[ o.currency_id and o.currency_id.symbol or '' ]] [[ formatLang(l.price_subtotal, dp='Account') ]]</para>
          </td>
        </tr>
      </blockTable>
    </section>
    <spacer length="0.5cm"/>
