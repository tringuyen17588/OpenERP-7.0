<?xml version="1.0"?>
<document filename="Invoices.pdf">
  <template title="Invoices" pageSize="(148mm,210mm)" author="OpenERP S.A.(sales@openerp.com)" allowSplitting="20">
    <pageTemplate id="first">
      <frame id="first" x1="3mm" y1="10mm" width="140mm" height="200mm"/>
	  <header>
            <pageGraphics>
                <setFont name="Helvetica-Bold" size="9"/>
				<paraStyle name="main_footer" fontSize="6.5" alignment="CENTER"/>
                <!-- For Header -->
				<image x="5.0cm" y="18.7cm" height="60.0" width="120.0">[[ company.logo or removeParentNode('image') ]]</image>
				<!-- For Remark -->
				<!--rotate degrees="90"/>
				<fill color="grey"/>
				<drawString x="0.65cm" y="-0.4cm"> [[ objects[0].comment and objects[0].comment or ' ' ]] </drawString>
				<fill color="black"/>
				<rotate degrees="-90"/-->
				<!-- For Footer -->
				<lines>1cm 2.45cm 13.2cm 2.45cm</lines>
				<place x="1cm" y="0cm" height="2cm" width="13.0cm">
					<para style="main_footer"> All cheques should be crossed and made payable to DERICK PTE LTD </para>
				</place>
            </pageGraphics>
        </header>
    </pageTemplate>
  </template>
