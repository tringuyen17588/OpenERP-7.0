<?xml version="1.0"?>
<document filename="Packing List.pdf">
  <template title="Income Shipment" author="Crea8s(info@crea8s.com)" allowSplitting="20" pageSize="(21.0cm, 29.0cm)">
    <!--pageTemplate id="first">
      <frame id="first" x1="0" y1="8.0" width="20cm" height="28.0cm"/>
    </pageTemplate-->
    <pageTemplate id="first">
      <frame id="first" x1="0" y1="8.0" width="20cm" height="28.0cm"/>
	  <header>
            <pageGraphics>
                <setFont name="Helvetica-Bold" size="9"/>
				<paraStyle name="main_footer" fontSize="8.0" alignment="CENTER"/>
                <!-- For Header -->
				<image x="7.0cm" y="26.7cm" height="60.0" width="120.0">[[ company.logo or removeParentNode('image') ]]</image>
				<!-- For Remark -->
				<!--rotate degrees="90"/>
				<fill color="grey"/>
				<drawString x="2.65cm" y="-0.4cm"> [[ objects[0].comment and objects[0].comment or ' ' ]] </drawString>
				<fill color="black"/>
				<rotate degrees="-90"/-->
				<!-- For Footer -->
				<lines>1.2cm 2.65cm 19.9cm 2.65cm</lines>
				<place x="1.3cm" y="0cm" height="2.55cm" width="19.0cm">
					<para style="main_footer"> All cheques should be crossed and made payable to DERICK PTE LTD </para>
				</place>
            </pageGraphics>
        </header>
    </pageTemplate>
  </template>
