  <story>
  <pto>
	
    <para style="terp_default_8">[[repeatIn(objects,'picking')]] </para>
    <para style="terp_default_8">[[picking.type == 'out' and picking.partner_id and setLang(picking.partner_id.lang) or '']]</para>
    <para style="terp_default_9">
      <font color="white"> </font>
    </para>
    <spacer length="2.0cm"/>
	<blockTable colWidths="139.0,269.0,130.0" style="Table_Address_detail">
      <tr>
        <td>
          <para style="terp_default_9"> [[ picking.min_date and time.strftime('%d-%m-%Y',time.strptime(picking.min_date, '%Y-%m-%d %H:%M:%S')) or ' ']] </para>
		  <para style="terp_default_9"> [[ picking.min_date and time.strftime('%H:%M:%S',time.strptime(picking.min_date, '%Y-%m-%d %H:%M:%S')) or ' ']] </para>
        </td>
        <td>
          <para style="terp_header"> Inventory Count Sheet </para>
        </td>
		<td>
			<para style="terp_default_9"> Page: <pageNumber/> </para>
        </td>
      </tr>
    </blockTable>

    <blockTable colWidths="56.0,220.0,73.0,59.0,59.0,71.0" repeatRows="1" style="Move_Line_Header">
      <tr>
        <td>
          <para style="terp_tblheader_Details">Item #</para>
        </td>
        <td>
          <para style="terp_tblheader_Details">Item Name</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Centre">Quantity</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Centre">Qty On Hand</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Right">Type Of Pack</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Right">Qty Pack</para>
        </td>
      </tr>
    </blockTable>
    <section>
      <para style="terp_default_2">[[ repeatIn([line for line in picking.move_lines if ((line.state == 'confirmed' or line.state=='done' or line.state=='assigned') and not line.scrapped)],'move_lines') ]]</para>
      <para style="terp_default_2">[[ (picking.move_lines!=[] and removeParentNode('para')) or removeParentNode('section')]]</para>
      <blockTable colWidths="56.0,200.0,74.0,58.0,60.0,73.0" style="Move_Line_Contect_Assign_State">
        <tr>
          <td>
            <para style="terp_default_9"><font face="Helvetica" size="9.0">[[ get_product_desc1(move_lines) ]] </font></para>
          </td>
		  <td>
            <para style="terp_default_9"><font face="Helvetica" size="9.0">[[ get_product_desc2(move_lines) ]] </font></para>
          </td>
          <td>
            <para style="terp_default_Centre_9">[[ formatLang(move_lines.product_qty) ]]</para>
          </td>
          <td>
            <para style="terp_default_9">Waiting Availability[[ move_lines.state == 'confirmed' and ' ' or removeParentNode('para') ]]</para>
            <para style="terp_default_9">Done[[ move_lines.state == 'done' and ' ' or removeParentNode('para') ]]</para>
            <para style="terp_default_9">Available[[ move_lines.state == 'assigned' and ' ' or removeParentNode('para') ]]</para>
          </td>
          <td>
            <para style="terp_default_Right_9">[[ (move_lines.location_id and move_lines.location_id.name) or '' ]] </para>
          </td>
          <td>
            <para style="terp_default_Right_9"> [[ move_lines.product_uom.name ]]</para>
          </td>
        </tr>
      </blockTable>
    </section>

    <section>
      <para style="terp_default_2">[[ repeatIn([line for line in picking.move_lines if (line.state == 'draft' or line.state=='waiting')],'move_lines') ]]</para>
      <para style="terp_default_2">[[ (picking.move_lines!=[] and removeParentNode('para')) or removeParentNode('section')]]</para>
      <blockTable colWidths="56.0,200.0,74.0,57.0,61.0,72.0" style="Table1">
        <tr>
          <td>
            <para style="terp_default_9"><font face="Helvetica" size="9.0">[[ get_product_desc1(move_lines) ]] </font></para>
          </td>
          <td>
            <para style="terp_default_9"><font face="Helvetica" size="9.0">[[ get_product_desc2(move_lines) ]] </font></para>
          </td>
          <td>
            <para style="terp_default_Centre_9">[[ formatLang(move_lines.product_qty) ]]</para>
          </td>
          <td>
            <para style="terp_default_9">[[ get_qty_onhand(move_lines.product_id and move_lines.product_id.id or 0, move_lines.location_dest_id.id) ]]</para>
          </td>
          <td>
            <section>
				  <para style="terp_default_2">[[ repeatIn(get_others_qty(move_lines.product_id and move_lines.product_id.id or 0, move_lines.product_qty),'line_if1') ]]</para>
				  <blockTable colWidths="70.0" style="Move_Line_Contect_Assign_State">
					<tr>
						<td>
							<para style="terp_default_Right_9"> [[ line_if1['name'] and line_if1['name'] or removeParentNode('blockTable') ]]</para>
						</td>
					</tr>
				  </blockTable>
			</section>
          </td>
          <td>
            <section>
				  <para style="terp_default_2">[[ repeatIn(get_others_qty(move_lines.product_id and move_lines.product_id.id or 0, get_qty_onhand(move_lines.product_id and move_lines.product_id.id or 0, move_lines.location_dest_id.id)),'line_if') ]]</para>
				  <blockTable colWidths="70.0" style="Move_Line_Contect_Assign_State">
					<tr>
						<td>
							<para style="terp_default_Right_9"> [[ line_if['qty_box'] and line_if['qty_box'] or removeParentNode('blockTable') ]]</para>
						</td>
					</tr>
				  </blockTable>
			</section>
          </td>
        </tr>
      </blockTable>
    </section>
    <para style="terp_default_2">
      <font color="white"> </font>
    </para>
