<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		<!-- domain="[('state','!=','draft')]" context="{'search_default_partner_id':partner_id}" -->
		<record id="view_company_form_id" model="ir.ui.view">
			<field name="name">crea8s_vector_quo.view_company_form</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='company_registry']" position="after">
					
					<field name="crea8s_certificate" widget="image"/>
				</xpath>
			</field>
		</record> 
		
		
    </data>
</openerp>
